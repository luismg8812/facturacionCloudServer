
<configuration>
  
  <!-- ID of the service. It should be unique accross the Windows system-->
  <id>fileWatcher</id>
  <!-- Display name of the service -->
  <name>fileWatcher (effective software)</name>
  <!-- Service description -->
  <description>componente que controla la impresiones</description>
  
  <!-- Path to the executable, which should be started -->
  <executable>java</executable>
<arguments>-jar %BASE%\fileWatcher.jar</arguments>
</configuration>